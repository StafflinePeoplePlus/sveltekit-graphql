{
	"name": "sveltekit-graphql",
	"version": "0.4.0",
	"type": "module",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/StafflinePeoplePlus/sveltekit-graphql"
	},
	"bugs": {
		"url": "https://github.com/StafflinePeoplePlus/sveltekit-graphql/issues"
	},
	"bin": {
		"sveltekit-graphql": "./dist/cli.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./vite": {
			"types": "./dist/vite.d.ts",
			"import": "./dist/vite.js"
		},
		"./config": {
			"types": "./dist/config.d.ts",
			"import": "./dist/config.js"
		}
	},
	"types": "./dist/index.d.ts",
	"typesVersions": {
		"*": {
			"index.d.ts": [
				"dist/index.d.ts"
			],
			"vite": [
				"dist/vite.d.ts"
			],
			"config": [
				"dist/config.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"bin"
	],
	"scripts": {
		"build": "tsc",
		"format": "prettier --write .",
		"release": "npm run build && changeset publish"
	},
	"dependencies": {
		"@changesets/cli": "^2.26.2",
		"@envelop/extended-validation": "^4.0.0",
		"@graphql-codegen/add": "^5.0.0",
		"@graphql-codegen/core": "^4.0.0",
		"@graphql-codegen/graphql-modules-preset": "^4.0.1",
		"@graphql-codegen/typescript": "^4.0.1",
		"@graphql-codegen/typescript-resolvers": "^4.0.1",
		"@graphql-tools/graphql-file-loader": "^8.0.0",
		"@graphql-tools/load": "^8.0.0",
		"@graphql-tools/load-files": "^7.0.0",
		"@graphql-tools/merge": "^9.0.0",
		"commander": "^11.1.0",
		"graphql": "^16.8.1",
		"graphql-yoga": "^5.0.0",
		"houdini": "^1.2.32",
		"houdini-svelte": "^1.2.32",
		"typescript": "^5.2.2"
	},
	"devDependencies": {
		"@types/node": "^20.8.10",
		"prettier": "^3.0.3",
		"vite": "^5.0.2"
	},
	"peerDependencies": {
		"vite": "^4.0.0"
	}
}
